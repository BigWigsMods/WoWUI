	<Frame name="EventToastManagerFrame" inherits="ResizeLayoutFrame" mixin="EventToastManagerFrameMixin" toplevel="true" parent="UIParent" hidden="true" frameStrata="HIGH">
		<KeyValues>
			<KeyValue key="fixedWidth" value="418" type="number"/>
			<KeyValue key="minimumHeight" value="72" type="number"/>
		</KeyValues>
		<Anchors>
			<Anchor point="TOP" x="0" y="-190"/>
		</Anchors>
		<Animations>
			<AnimationGroup parentKey="fadeIn">
				<Alpha fromAlpha="0" toAlpha="1" duration="0.5" order="1" parentKey="anim1" />
				<Scripts>
					<OnFinished>
						self:GetParent():PlayAnim();
					</OnFinished>
				</Scripts>
			</AnimationGroup>
			<AnimationGroup parentKey="hideAnim">
				<Alpha fromAlpha="1" toAlpha="0" duration="1.0" order="1" />
				<Scripts>
					<OnFinished>
						self:GetParent():ToastingEnded();
					</OnFinished>
				</Scripts>
			</AnimationGroup>
			<AnimationGroup parentKey="fastHide">
				<Alpha fromAlpha="1" toAlpha="0" duration=".5" order="1" />
				<Scripts>
					<OnFinished>
						self:GetParent():ToastingEnded();
					</OnFinished>
				</Scripts>
			</AnimationGroup>
		</Animations>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="BlackBG" atlas="levelup-shadow-upper" useAtlasSize="true">
					<KeyValues>
						<KeyValue key="ignoreInLayout" value="true" type="boolean"/>
					</KeyValues>
					<Animations>
						<AnimationGroup parentKey="grow">
							<Scale scaleX="1.0" scaleY="0.001" duration="0.0" startDelay="1.5" order="1" parentKey="anim1">
								<Scripts>
									<OnFinished>
										self:GetParent():GetParent():Show();
									</OnFinished>
								</Scripts>
								<Origin point="BOTTOM">
									<Offset x="0" y="0"/>
								 </Origin>
							</Scale>
							<Scale scaleX="1.0" scaleY="1000.0" startDelay="0.25" duration="0.15" order="2">
								 <Origin point="BOTTOM">
									<Offset x="0" y="0"/>
								 </Origin>
							</Scale>
						</AnimationGroup>
					</Animations>
					<Anchors>
						<Anchor point="BOTTOM" x="0" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BACKGROUND" textureSubLevel="2">
				<Texture parentKey="GLine2" atlas="levelup-bar-gold" useAtlasSize="true">
					<KeyValues>
						<KeyValue key="ignoreInLayout" value="true" type="boolean"/>
					</KeyValues>
					<Anchors>
						<Anchor point="TOP" x="0" y="0"/>
					</Anchors>
					<Animations>
						<AnimationGroup parentKey="grow">
							<Scale scaleX="0.001" scaleY="1.0" duration="0.0" startDelay="1.5" order="1" parentKey="anim1">
								<Scripts>
									<OnFinished>
										self:GetParent():GetParent():Show();
									</OnFinished>
								</Scripts>
							</Scale>
							<Scale scaleX="1000.0" scaleY="1.0" duration="0.5" order="2"/>
						</AnimationGroup>
					</Animations>
				</Texture>
				<Texture parentKey="GLine" atlas="levelup-bar-gold" useAtlasSize="true">
					<KeyValues>
						<KeyValue key="ignoreInLayout" value="true" type="boolean"/>
					</KeyValues>
					<Anchors>
						<Anchor point="BOTTOM" x="0" y="0"/>
					</Anchors>
					<Animations>
						<AnimationGroup parentKey="grow">
							<Scale scaleX="0.001" scaleY="1.0" duration="0.0" startDelay="1.5" order="1" parentKey="anim1">
								<Scripts>
									<OnFinished>
										self:GetParent():GetParent():Show();
									</OnFinished>
								</Scripts>
							</Scale>
							<Scale scaleX="1000.0" scaleY="1.0" duration="0.5" order="2"/>
							<Scripts>
								<OnPlay>
									local parent = self:GetParent():GetParent();
									parent.BlackBG:Hide();
									parent.GLine:Hide();
									parent.GLine2:Hide();
									parent.GLine2.grow:Play();
									parent.BlackBG.grow:Play();
								</OnPlay>
							</Scripts>
						</AnimationGroup>
					</Animations>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEvent method="OnEvent"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
		</Scripts>
	</Frame>

